{
    "name": "Leap Tab",
    "manifest_version": 2,
    "version": "0.0.1",
    "description": "キーボードでタブ移動する",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["underscore.js", "favicon.min.js", "leapTab.js"]
        }
    ],
    "background": {
        "scripts": [
            "background.js",
            "underscore.js"
        ]
    },
    "permissions": [
        "tabs"
    ]
}
