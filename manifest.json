{
    "name": "Leap Tab",
    "manifest_version": 2,
    "version": "0.1.0",
    "description": "キーボードでタブ移動する",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["leapTab.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "options_page": "options.html",
    "permissions": ["tabs", "storage"],
    "web_accessible_resources": [
        "favicon/*"
    ]
}
