{
    "name": "Leap Tab",
    "manifest_version": 2,
    "version": "0.1.2",
    "description": "You can leap tab to tab by keyboard with this extension.",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["common.js", "leapTab.js"]
        }
    ],
    "background": {
        "scripts": ["common.js", "background.js"]
    },
    "options_page": "options.html",
    "permissions": ["tabs", "storage"],
    "web_accessible_resources": [
        "favicon/*"
    ]
}
